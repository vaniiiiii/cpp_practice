#include <stdio.h>

void update(int *a,int *b) {
    int sum = *a+ *b;
     int mult;
    if(*a>*b)
    {
         mult = *a-*b;
    }  
    else
    {
        mult = *b-*a;
    }
     *a = sum;
   *b = mult;
    // Complete this function    
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a, b);

    return 0;
}